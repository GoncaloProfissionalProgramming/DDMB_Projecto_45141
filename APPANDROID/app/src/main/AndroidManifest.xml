<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.goncalo.tpcf">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login"
            android:label="@string/app_login" />
        <activity
            android:name=".Registar"
            android:label="@string/app_registar" />
        <activity
            android:name=".ForgotPass"
            android:label="@string/app_forgotPass" />
        <activity
            android:name=".PatraoOpt"
            android:label="@string/app_PatraoOpt" />
        <activity
            android:name=".empresa"
            android:label="@string/app_Empresa" />
        <activity
            android:name=".empregadoOpt"
            android:label="@string/app_EmpregadoOpt" />
        <activity
            android:name=".empregados"
            android:label="@string/app_Empregados" />
        <activity android:name=".RegistarCargas"></activity>
    </application>

</manifest>